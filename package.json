{
  "name": "supermercado-backend",
  "version": "1.0.0",
  "description": "Backend API para sistema de supermercado",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:create": "psql -U postgres -f scripts/createTables.sql",
    "db:seed": "psql -U postgres -d supermercado -f scripts/seed.sql",
    "create-admin": "node scripts/createAdmin.js"
  },
  "keywords": [
    "api",
    "supermercado",
    "ecommerce"
  ],
  "author": "Seu Nome",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
